{
  "name": "drizzle-uncache",
  "version": "0.0.1",
  "description": "Cache adapter for Drizzle ORM powered by unstorage",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dschewchenko/drizzle-uncache.git"
  },
  "author": "dschewchenko",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunx unbuild",
    "check": "biome check .",
    "check:fix": "biome check . --write",
    "dev": "bunx vitest",
    "format": "biome format . --write",
    "lint": "biome lint .",
    "playground": "bun playground/redis.ts",
    "prepack": "bun run build",
    "release": "semantic-release",
    "test": "bun run check && bun run test:types && bunx vitest run",
    "test:types": "bunx tsc --noEmit --pretty false"
  },
  "peerDependencies": {
    "drizzle-orm": "^0.45.1",
    "unstorage": "^1.17.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.3",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/bun": "^1.3.4",
    "@types/node": "^25.0.1",
    "@vitest/coverage-v8": "^4.0.15",
    "drizzle-orm": "^0.45.1",
    "elysia": "^1.4.19",
    "ioredis": "^5.8.2",
    "semantic-release": "^25.0.2",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "unstorage": "^1.17.3",
    "vitest": "^4.0.15"
  },
  "publishConfig": {
    "provenance": true
  },
  "engines": {
    "node": ">=22",
    "bun": ">=1.0.0"
  },
  "packageManager": "bun@1.3.1"
}
